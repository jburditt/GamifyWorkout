{
  "name": "gamifyworkout",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "run-script-os",
    "genapi": "ng-openapi-gen --input https://localhost:8080/swagger/v1/swagger.json --output src/app/api --exclude-tags Metadata",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "prestart": "node aspnetcore-https",
    "start:windows": "ng s --port 4200 --ssl --ssl-cert \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem\" --ssl-key \"%APPDATA%\\ASP.NET\\https\\%npm_package_name%.key\" --host=127.0.0.1",
    "start:default": "ng s --port 4200 --ssl --ssl-cert \"$HOME/.aspnet/https/${npm_package_name}.pem\" --ssl-key \"$HOME/.aspnet/https/${npm_package_name}.key\" --host=127.0.0.1",
    "deploy": "swa deploy ./dist/gamifyworkout/browser --app-name stapp-gamifyworkout --env production",
    "deploy:local": "swa start ./dist/gamifyworkout/browser",
    "storybook": "ng run gamifyworkout:storybook",
    "build-storybook": "ng run gamifyworkout:build-storybook"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^21.1.2",
    "@angular/cdk": "^21.1.2",
    "@angular/common": "^21.1.2",
    "@angular/compiler": "^21.1.2",
    "@angular/core": "^21.1.2",
    "@angular/forms": "^21.1.2",
    "@angular/material": "^21.1.2",
    "@angular/platform-browser": "^21.1.2",
    "@angular/platform-browser-dynamic": "^21.1.2",
    "@angular/router": "^21.1.2",
    "@ngrx/store": "^20.0.1",
    "@ngworker/lumberjack": "^21.0.1",
    "@ngxpert/hot-toast": "^5.1.1",
    "angular-oauth2-oidc": "^17.0.1",
    "fullswing-angular-library": "file:../../fullswing-angular/dist/fullswing-angular-library",
    "jest-editor-support": "*",
    "ngx-ui-loader": "^19.0.0",
    "odata-query": "^8.0.4",
    "run-script-os": "*",
    "rxjs": "~7.8.0",
    "survey-angular-ui": "^2.3.5",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular/build": "^21.1.2",
    "@angular/cli": "^20.0.2",
    "@angular/compiler-cli": "^21.1.2",
    "@azure/static-web-apps-cli": "^2.0.7",
    "@compodoc/compodoc": "^1.1.23",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "ng-openapi-gen": "^0.53.0",
    "parse5": "^7.2.0",
    "typescript": "~5.9.3"
  }
}
